{
    "name": "hyper-ui",
    "description": "A lightweight front-end UI lib.",
    "version": "0.1.0",
    "main": "dist/hyper-ui.umd.min.js",
    "module": "dist/hyper-ui.min.js",
    "types": "index.d.ts",
    "scripts": {
        "prebuild": "rd /q /s dist typings raw || exit 0",
        "build": "tsc && rollup -c",
        "postbuild": "terser dist/hyper-ui.js --config-file terser.json --module -o dist/hyper-ui.min.js && terser dist/hyper-ui.umd.js --config-file terser.json -o dist/hyper-ui.umd.min.js"
    },
    "license": "MIT",
    "author": "3h",
    "repository": "huang2002/hyper-ui",
    "bugs": {
        "url": "https://github.com/huang2002/hyper-ui/issues"
    },
    "homepage": "https://github.com/huang2002/hyper-ui#readme",
    "keywords": [
        "HUI",
        "front-end",
        "virtual-dom",
        "vdom",
        "lightweight",
        "define",
        "reusable",
        "component",
        "simple",
        "easy",
        "UI",
        "lib",
        "3h"
    ],
    "files": [
        "dist",
        "typings",
        "index.d.ts"
    ],
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@babel/preset-env": "^7.1.0",
        "rollup": "^0.67.0",
        "rollup-plugin-babel": "^4.0.3",
        "terser": "^3.10.11",
        "typescript": "^3.1.6"
    }
}